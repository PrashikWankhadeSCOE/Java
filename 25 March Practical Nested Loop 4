===================================== QUE 7 ===========================================

F
E 1
D 2 E
C 3 D 4
B 5 C 6 D
A 7 B 8 C 9

class MyClass{
        public static void main(String [] args ){
                int N = 6;
                int x = 1;
                char ch = 'F';
                for(int i = 1; i<=N; i++){
                        char ch1 = ch;
                        for(int j = 1; j<=i; j++){
                                if(j%2==1){
                                        System.out.print(ch1++ + " ");
                                }
                                else{
                                        System.out.print(x++ +" ");
                                }
                        }
                        ch--;
                        System.out.println();
                }
        }
}

===================================== QUE 8 ===========================================

10
I H
7 6 5
D C B A

class MyClass{
        public static void main(String [] args ){
                int N = 4;
                int x = 10;
                char ch = 'J';
                for(int i = 1; i<=N; i++){
                        for(int j = 1; j<=i; j++){
                                if(i%2==0){
                                        System.out.print(ch + " ");
                                }
                                else{
                                        System.out.print(x +" ");
                                }
                                ch--;
                                x--;
                        }
                        System.out.println();
                }
        }
}

===================================== QUE 9 ===========================================

1
8 9
27 16 125
64 25 216 49

class MyClass{
        public static void main(String [] args ){
                int N = 4;

                for(int i = 1; i<=N; i++){
                        int x = i;
                        for(int j = 1; j<=i; j++){
                                if(j%2==1){
                                        System.out.print(x*x*x + " ");
                                }
                                else{
                                        System.out.print(x*x +" ");

                                }
                                x++;
                        }
                        System.out.println();
                }
        }
}

===================================== QUE 10 ===========================================

1
8 9
9 64 25
64 25 216 49

class MyClass{
        public static void main(String [] args ){
                int N = 4;
                for(int i = 1; i<=N; i++){
                        int x = i;
                        for(int j = 1; j<=i; j++){
                                if(x%2==0){
                                        System.out.print(x*x*x + " ");
                                }
                                else{
                                        System.out.print(x*x +" ");
                                }
                                x++;
                        }
                        System.out.println();
                }
        }
}



























