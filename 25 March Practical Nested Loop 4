===================================== QUE 4 ===========================================

1 2 3 4 5
2 3 4 5
3 4 5
4 5
5

class MyClass{
        public static void main(String [] args ){
                int N = 5;

                for(int i = 1; i<=N; i++){
                        int x = i;
                        for(int j = 1; j<=N-i+1; j++){
                                System.out.print(x++ +" ");
                        }
                        System.out.println();
                }
        }
}

===================================== QUE 5 ===========================================
A B C D
B C D
C D
D

class MyClass{
        public static void main(String [] args ){
                int N = 4;
                char ch = 'A';
                for(int i = 1; i<=N; i++){
                        char ch1 = ch;
                        for(int j = 1; j<=N-i+1; j++){
                                System.out.print(ch1++ +" ");

                        }
                        ch++;
                        System.out.println();
                }
        }
}

