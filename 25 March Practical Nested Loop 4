===================================== QUE 1 ===========================================

C2W10
C2W9 C2W8
C2W7 C2W6 C2W5
C2W4 C2W3 C2W2 C2W1

class MyClass{
        public static void main(String [] args ){
                int N = 4;
                int x = 10;
                for(int i = 1; i<=N; i++){
                        for(int j = 1; j<=i; j++){
                                System.out.print("C2W"+ x-- +" ");
                        }
                        System.out.println();
                }
        }
}

===================================== QUE 2 ===========================================

1
3 4
6 7 8
10 11 12 13
15 16 17 18 19

class MyClass{
        public static void main(String [] args ){
                int N = 5;
                int x = 1;
                for(int i = 1; i<=N; i++){
                        for(int j = 1; j<=i; j++){
                                System.out.print(x++ +" ");
                        }
                        x++;
                        System.out.println();
                }
        }
}

